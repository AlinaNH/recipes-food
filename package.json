{
  "name": "nestjs-react-template",
  "version": "0.0.1",
  "description": "Recipes-food",
  "author": "enerGOww <enerGOww@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "pm2": "pm2",
    "build:front": "webpack --mode development",
    "build:front:prod": "webpack-dev-server --mode production --config webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"front/**/*.tsx\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:front": "webpack --watch --mode development",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/front",
    "typeorm:run": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:run",
    "typeorm:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate -n",
    "typeorm:revert": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "test": "jest src/ --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest src/ --watch --runInBand --detectOpenHandles --forceExit",
    "test:cov": "jest src/ --coverage --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --forceExit",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/typeorm": "^7.1.4",
    "@types/react-bootstrap-table": "^4.3.8",
    "@types/react-bootstrap-table-next": "^4.0.10",
    "@types/react-bootstrap-table2-paginator": "^2.1.1",
    "@types/react-bootstrap-table2-toolkit": "^2.1.4",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "framer-motion": "^2.9.4",
    "lodash": "^4.17.19",
    "pg": "^8.3.3",
    "react-bootstrap-table": "^4.3.1",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-bootstrap-table2-toolkit": "^2.1.3",
    "react-icons": "^3.11.0",
    "react-slick": "^0.27.11",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "slick-carousel": "^1.8.1",
    "stripe": "^8.56.0",
    "typeorm": "^0.2.27"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@types/express": "^4.17.3",
    "@types/got": "^9.6.11",
    "@types/jest": "^25.2.3",
    "@types/material-ui": "^0.21.8",
    "@types/node": "^13.9.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-slick": "^0.23.4",
    "@types/stripe": "^7.13.23",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "autoprefixer": "^9.8.6",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "css-loader": "^4.3.0",
    "eslint": "^7.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "file-loader": "^6.1.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^7.0.1",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.7",
    "mobx": "^5.15.7",
    "mobx-react": "^6.3.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^4.0.3",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "style-loader": "^1.3.0",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "roots": [
      "src",
      "front"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "preset": "ts-jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint \"**/*.{ts,tsx}\" --fix",
    "*.tsx": "eslint \"**/*.{ts,tsx}\" --fix"
  },
  "browserslist": [
    "last 2 version",
    "not dead",
    ">0.1%"
  ],
  "resolutions": {
    "@types/react": "^17.0.2"
  }
}
